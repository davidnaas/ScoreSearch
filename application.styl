@import 'jeet/index'



form
	align(b)
	
p
	font-size 2.5em
	opacity 0
	font-family: 'Raleway'
	font-weight 700

.scoreImg
	max-height 1000px
	max-width 700px
	col(6/8)

.space-frame
	i
		opacity 0
		padding 10px
		display block
		position relative
		z-index -1 !important
	.fa-file-pdf-o
		margin-top 50px	
	
input
	font-family 'Raleway', sans-serif
	font-size 2em

.searchButton
	border-radius 40px
	padding 12px 20px
	border solid 3px #aaa
	background-color: #FFF
	
.searchButton:hover
	cursor pointer

.searchField
	border-radius 40px
	padding 15px 20px
	border solid 3px #aaa
	outline none

.cornerSearch
	opacity 0
	position fixed
	top 20px
	right 50px
	color #555
	z-index: 9999 !important

.visibleSearch
	opacity 1
	position relative
	font-size 3em
	padding 6px 20px
	color #555

	
.logodiv
	position fixed
	top 20px
	left 20px
	width 500px
	img
		opacity 1
		height 30px
		padding-right 10px
	h1
		display inline
		font-size 3.1em
		font-family 'Raleway', sans-serif
		font-weight 100

		
.fa
	cursor pointer !important
	
img
	opacity 0
	
a
	color black
	text-decoration none

a:visited
	color black
	
.spacediv
	width 100vh
	height 90vh
	


















	

	
