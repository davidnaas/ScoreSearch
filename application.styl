@import 'jeet/index'



form
	align(b)
	
p
	font-size 2em
	opacity 0

img
	max-height 1000px
	max-width 500px
	col(6/8)

.space-frame
	i
		opacity 0
		padding 10px
		display block
		position relative
		z-index -1 !important
	.fa-file-pdf-o
		margin-top 50px	
	
input
	font-family "Avenir"
	font-size 2em

.searchButton
	border-radius 40px
	padding 12px 20px
	border solid 3px #aaa
	background-color: #FFF
	
.searchButton:hover
	cursor pointer

.searchField
	border-radius 40px
	padding 12px 20px
	border solid 3px #aaa
	outline none

.fa-search
	opacity 0
	position fixed
	top 20px
	right 50px
	color #555
	z-index: 9999 !important

		
.fa
	cursor pointer !important
	
img
	opacity 0
	
a
	color black

a:visited
	color black
	
.spacediv
	width 100vh
	height 90vh
	


















	

	
